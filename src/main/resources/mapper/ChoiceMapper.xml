<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qst.mapper.ChoiceMapper">


    <insert id="insertChoice" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into choices(question_id,title,hint,checked) values (#{question_id},#{title},#{hint},#{checked})
    </insert>


</mapper>